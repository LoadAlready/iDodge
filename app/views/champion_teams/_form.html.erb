
<%= form_tag users_path do %>
  <% @users_arr.each do |user| %>
    <%= fields_for 'users_container[]', user do |p| %>
        Summoner Name: <%= p.text_field :name %>
        Team Champ: <%= fields_for 'champ_arr[]', ChampionTeam.new do |f| %>
          <%= f.collection_select( :champion_id, Champion.all , :id, :name)%>
          <% end %> |
          Opposing Champ: <%= fields_for 'opp_champ_arr[]', ChampionTeam.new do |f| %>
            <%= f.collection_select( :champion_id, Champion.all , :id, :name)%></br>
        <% end %>
    <% end %>
  <% end %>
    <%= submit_tag %>
<% end %>
